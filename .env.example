# OpenWeatherMap API Key
# Get yours at: https://openweathermap.org/api/one-call-3
VITE_OPENWEATHERMAP_API_KEY=your_api_key_here

# Mapbox Access Token
# Get yours at: https://account.mapbox.com/
# Public token for app runtime (pk.eyJ1...)
VITE_MAPBOX_ACCESS_TOKEN=your_mapbox_public_token_here

# Firebase Configuration
# Get yours at: https://console.firebase.google.com/
VITE_FIREBASE_API_KEY=your_firebase_api_key
VITE_FIREBASE_AUTH_DOMAIN=your-project.firebaseapp.com
VITE_FIREBASE_PROJECT_ID=your-project-id
VITE_FIREBASE_STORAGE_BUCKET=your-project.appspot.com
VITE_FIREBASE_MESSAGING_SENDER_ID=123456789
VITE_FIREBASE_APP_ID=1:123456789:web:abc123def456

# Optional: Firebase Auth Emulator (for local development)
# VITE_FIREBASE_AUTH_EMULATOR_HOST=localhost:9099
